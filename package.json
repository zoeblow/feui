    "gulp-cssmin": "^0.2.0",
    "gulp-postcss": "^7.0.0",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.30.1",
    "isparta-loader": "^2.0.0",
    "json-templater": "^1.1.0",
    "karma": "^1.7.1",
