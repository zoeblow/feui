    "extract-text-webpack-plugin": "^2.0.0",
    "fast-vue-md-loader": "^1.0.2",
    "file-loader": "^1.1.6",
    "file-save": "^0.2.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^4.0.2",
